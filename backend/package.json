{
  "name": "cancha-a-la-vista-backend",
  "version": "1.0.0",
  "description": "Backend para Cancha a la Vista - Sistema de reservas de canchas deportivas",
  "main": "src/server.js",
  "scripts": {
    "start": "exec node src/server.js",
    "dev": "nodemon src/server.js",
    "migrate": "node src/database/migrations/run_migration.js",
    "seed": "node src/database/seeds/seed.js",
    "seed:complete": "node src/database/seeds/seed_complete.js",
    "setup": "node src/database/setup_supabase.js",
    "db:reset": "node src/database/setup_supabase.js && node src/database/seeds/seed_complete.js",
    "verify": "node src/scripts/verify-config.js"
  },
  "keywords": [
    "sports",
    "booking",
    "reservation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.78.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
